<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="130dp"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/Image1"
            android:layout_width="130dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:src="@drawable/bts_1" />

        <ImageView
            android:id="@+id/Image2"
            android:layout_width="130dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:src="@drawable/bts_2"
            app:srcCompat="@drawable/KakaoTalk_20210909_221801718_01" />

        <ImageView
            android:id="@+id/Image3"
            android:layout_width="130dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:src="@drawable/bts_3"
            app:srcCompat="@drawable/KakaoTalk_20210909_221801718_02" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="130dp"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/Image4"
            android:layout_width="130dp"
            android:layout_height="130dp"
            android:layout_weight="1"
            android:src="@drawable/bts_4"
            app:srcCompat="@drawable/KakaoTalk_20210909_221801718_03" />

        <ImageView
            android:id="@+id/Image5"
            android:layout_width="130dp"
            android:layout_height="130dp"
            android:layout_weight="1"
            android:src="@drawable/bts_5"
            app:srcCompat="@drawable/KakaoTalk_20210909_221801718_04" />

        <ImageView
            android:id="@+id/Image6"
            android:layout_width="130dp"
            android:layout_height="130dp"
            android:rotation="90"
            android:src="@drawable/bts_6"
            app:srcCompat="@drawable/KakaoTalk_20210909_221801718_05"
            android:layout_weight="1" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="130dp"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="130dp"
            android:layout_height="130dp"
            android:layout_weight="1"
            android:rotation="90"
            app:srcCompat="@drawable/KakaoTalk_20210909_221801718_06" />

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="130dp"
            android:layout_height="130dp"
            android:layout_weight="1"
            app:srcCompat="@drawable/KakaoTalk_20210909_222318469" />

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="130dp"
            android:layout_height="130dp"
            android:layout_weight="1"
            app:srcCompat="@drawable/KakaoTalk_20210909_222318469_01" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="130dp"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="130dp"
            android:layout_height="130dp"
            android:layout_weight="1"
            app:srcCompat="@drawable/KakaoTalk_20210909_222318469_02" />

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="130dp"
            android:layout_height="130dp"
            android:layout_weight="1"
            app:srcCompat="@drawable/KakaoTalk_20210909_222318469_03" />

        <ImageView
            android:id="@+id/imageView7"
            android:layout_width="130dp"
            android:layout_height="130dp"
            android:layout_weight="1"
            android:rotation="90"
            app:srcCompat="@drawable/KakaoTalk_20210909_222318469_04" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="130dp"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/imageView8"
            android:layout_width="130dp"
            android:layout_height="130dp"
            android:layout_weight="1"
            app:srcCompat="@drawable/KakaoTalk_20210909_222318469_05" />

        <ImageView
            android:id="@+id/imageView9"
            android:layout_width="130dp"
            android:layout_height="130dp"
            android:layout_weight="1"
            app:srcCompat="@drawable/KakaoTalk_20210909_222318469_06" />

        <ImageView
            android:id="@+id/imageView10"
            android:layout_width="130dp"
            android:layout_height="130dp"
            android:layout_weight="1"
            app:srcCompat="@drawable/KakaoTalk_20210909_222318469_07" />
    </LinearLayout>

</LinearLayout>